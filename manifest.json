{
  "manifest_version": 3,
  "name": "force_delete",
  "version": "1.0",

  "description": "automatically types AWS's delete-safe words",

  "content_scripts": [
    {
      "matches": ["*://*.console.aws.amazon.com/*"],
      "js": ["main.js"],
      "all_frames": true,
      "match_about_blank": true,
      "match_origin_as_fallback": true
    }
  ]
}
